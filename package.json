{
  "name": "vue3-vite-boilerplate",
  "version": "1.0.0",
  "author": "Victor Neves <info@victorneves.dev>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorlmneves/vue3-vite-boilerplate"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victorlmneves/vue3-vite-boilerplate/issues"
  },
  "homepage": "https://victorneves.dev",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "lint:eslint": "yarn eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,ts,vue,html}\"",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "yarn prettier --check \"**/*.{js,json,ts,vue,html}\"",
    "lint:prettier:fix": "yarn prettier --write \"**/*.{js,json,ts,vue,html}\"",
    "lint:stylelint": "yarn stylelint --cache -q \"**/*.{vue,css}\"",
    "lint:stylelint:fix": "yarn lint:stylelint --fix",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:stylelint",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "typecheck": "vue-tsc --noEmit",
    "precommit": "yarn lint && yarn test && yarn build"
  },
  "dependencies": {
    "pinia": "^2.0.9",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.9",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.17",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "2.0.0-rc.16",
    "autoprefixer": "^10.4.2",
    "axios": "^0.24.0",
    "eslint": "^8.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.2.0",
    "happy-dom": "^2.25.2",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.2.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "~4.5.4",
    "vite": "^2.7.7",
    "vite-plugin-html-env": "^1.0.3",
    "vitest": "^0.1.13",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.29.8"
  },
  "keywords": [
    "vue",
    "vue3",
    "vite",
    "boilerplate",
    "Pinia",
    "Axios",
    "Vue-Router",
    "Jest",
    "Cypress",
    "Vue-TSC",
    "Vue-DevServer",
    "Vue-Boilerplate"
  ],
  "description": "Simple boilerplate for Vue3 projects using Vite, TypeScript, Vue-Router, Pinia, Axios, Jest and Cypress"
}
