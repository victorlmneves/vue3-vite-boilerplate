{
  "name": "vue3-vite-boilerplate",
  "version": "1.0.0",
  "author": "Victor Neves <info@victorneves.dev>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorlmneves/vue3-vite-boilerplate"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victorlmneves/vue3-vite-boilerplate/issues"
  },
  "homepage": "https://victorneves.dev",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "cypress open-ct",
    "test:unit:ci": "cypress run-ct --quiet --reporter spec",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "pinia": "^2.0.9",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.2",
    "@cypress/vue": "^3.1.0",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.17",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "axios": "^0.24.0",
    "cypress": "^9.2.0",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.5.4",
    "vite": "^2.7.7",
    "vue-tsc": "^0.29.8"
  },
  "keywords": [
    "vue",
    "vue3",
    "vite",
    "boilerplate",
    "Pinia",
    "Axios",
    "Vue-Router",
    "Jest",
    "Cypress",
    "Vue-TSC",
    "Vue-DevServer",
    "Vue-Boilerplate"
  ],
  "description": "Simple boilerplate for Vue3 projects using Vite, TypeScript, Vue-Router, Pinia, Axios, Jest and Cypress"
}
